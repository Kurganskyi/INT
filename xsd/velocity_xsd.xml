<?xml version="1.0" encoding="UTF-8"?>
<ЭДСФР
  xmlns="http://пф.рф/ВВ/МСК/МЗАЗ/2024-01-01"
  xmlns:МСК8="http://пф.рф/ВВ/МСК/2024-01-01"
  xmlns:АФ7="http://пф.рф/АФ/2023-04-03"
  xmlns:УТ7="http://пф.рф/УТ/2023-04-03"
>
	<МЗАЗ>
		<ТОСФР>
			<УТ7:Код>${form0.pfr.ut7_code}</УТ7:Код>
			<УТ7:Наименование>${form0.pfr.namePFR_txt}</УТ7:Наименование>
		</ТОСФР>
		<Заявитель>
			<УТ7:ФИО>
				<УТ7:Фамилия>${form0.InfoPanel.F_Applicant}</УТ7:Фамилия>
				<УТ7:Имя>${form0.InfoPanel.I_Applicant}</УТ7:Имя>
				<УТ7:Отчество>${form0.InfoPanel.Ot_Applicant}</УТ7:Отчество>
			</УТ7:ФИО>
			<МСК8:СтатусПоМСК>
				<МСК8:Код>
					#if (${form0.InfoPanel.stat}=="Мать")1
					#elseif (${form0.InfoPanel.stat}=="Отец")2
					#elseif (${form0.InfoPanel.stat}=="Ребенок")3
					#elseif (${form0.InfoPanel.stat}=="Мужчина - единственный усыновитель")4
					#elseif (${form0.InfoPanel.stat}=="Мужчина, воспитывающий детей")5
					#elseif (${form0.InfoPanel.stat}=="Мужчина - единственный родитель")6
					#elseif (${form0.InfoPanel.stat}=="Женщина - супруга мужчины, усыновившая ребенка (детей)")7
					#else
					0
					#end
				</МСК8:Код>
				<УТ7:Наименование>${form0.InfoPanel.stat}</УТ7:Наименование>
			</МСК8:СтатусПоМСК>
			<УТ7:ДатаРождения>${form0.InfoPanel.date_birth.ToString("yyyy-MM-dd")}</УТ7:ДатаРождения>
			<УТ7:УдостоверяющийДокумент>
				<УТ7:Код>#if (${form0.PasData.SelectTypeDoc}=="Паспорт гражданина РФ")27#{elseif ${form0.PasData.SelectTypeDoc}=="Свидетельство о рождении гражданина РФ")3#{else}0#end</УТ7:Код>
				<УТ7:Серия>${form0.PasData.Seria}</УТ7:Серия>
				<УТ7:Номер>${form0.PasData.Number}</УТ7:Номер>
				<УТ7:ДатаВыдачи>${form0.PasData.DatePas.ToString("yyyy-MM-dd")}</УТ7:ДатаВыдачи>
				<УТ7:КемВыдан>${form0.PasData.PassBy}</УТ7:КемВыдан>
			</УТ7:УдостоверяющийДокумент>
			<УТ7:СНИЛС>${form0.InfoPanel.snils}</УТ7:СНИЛС>
			<УТ7:АдресРФ>
				<УТ7:ТипАдреса>1</УТ7:ТипАдреса>
				<УТ7:Адрес>
					<УТ7:Индекс>${form0.InfoPanel.adr_reg_zayv_fias.PostalIndex}</УТ7:Индекс>
					<УТ7:Город>
						<УТ7:Название>${form0.InfoPanel.adr_reg_zayv_fias.CityName}</УТ7:Название>
					</УТ7:Город>
					<УТ7:Улица>
						<УТ7:Название>${form0.InfoPanel.adr_reg_zayv_fias.StreetName}</УТ7:Название>
						<УТ7:Сокращение>${form0.InfoPanel.adr_reg_zayv_fias.StreetShortName}</УТ7:Сокращение>
					</УТ7:Улица>
					<УТ7:Дом>${form0.InfoPanel.adr_reg_zayv_fias.House}</УТ7:Дом>
					<УТ7:Квартира>${form0.InfoPanel.adr_reg_zayv_fias.Flat}</УТ7:Квартира>
				</УТ7:Адрес>
			</УТ7:АдресРФ>
			<УТ7:Телефон>${form0.InfoPanel.phone}</УТ7:Телефон>
		</Заявитель>
		<РеквизитыСертификата>
			<МСК8:Серия>${form0.certificate_info.seria}</МСК8:Серия>
			<МСК8:Номер>${form0.certificate_info.number}</МСК8:Номер>
			<МСК8:ДатаВыдачи>${form0.certificate_info.issued_date.ToString("yyyy-MM-dd")}</МСК8:ДатаВыдачи>
			<МСК8:КемВыдан>
				<УТ7:Код>${form0.certificate_info.code}</УТ7:Код>
				<УТ7:Наименование>${form0.certificate_info.issued_by}</УТ7:Наименование>
			</МСК8:КемВыдан>
			<МСК8:Дубликат>#if (${form0.certificate_info.duplicate}=="True")1#{else}0#end</МСК8:Дубликат>
		</РеквизитыСертификата>
		<ДатаВозникновенияПраваНаДМГП>${form0.uk_n_v.date1.ToString("yyyy-MM-dd")}</ДатаВозникновенияПраваНаДМГП>
		<РеквизитыМЗРК>
			<УТ7:Дата>${form0.uk_n_v.date3.ToString("yyyy-MM-dd")}</УТ7:Дата>
			<УТ7:Номер>${form0.uk_n_v.reg_num}</УТ7:Номер>
		</РеквизитыМЗРК>
		<ДатаЗаполнения>${form0.pfr.date_z.ToString("yyyy-MM-dd")}</ДатаЗаполнения>
	</МЗАЗ>
	<СлужебнаяИнформация>
		<АФ7:GUID>${FileInfo.FileId}</АФ7:GUID>
		<АФ7:ДатаВремя>${DateNow.substring(0,19)}+${DateNow.substring(28,5)}</АФ7:ДатаВремя>
		<МСК8:СоставительСФР>
			<УТ7:Код>${form0.pfr.code}</УТ7:Код>
			<УТ7:Наименование>${form0.pfr.namePFR_txt}</УТ7:Наименование>
		</МСК8:СоставительСФР>
		<МСК8:ТипФормы>1</МСК8:ТипФормы>
		<МСК8:ДатаПодачи>${DateNow.substring(0,10)}</МСК8:ДатаПодачи>
		<Реквизиты>
			<УТ7:Дата>${form0.pfr.date_z.ToString("yyyy-MM-dd")}</УТ7:Дата>
			<УТ7:Номер>${form0.pfr.reg_n}</УТ7:Номер>
		</Реквизиты>
		<Специалист>
			<УТ7:Фамилия>${DepartmentConfig.DepartmentWorkerLastName}</УТ7:Фамилия>
			<УТ7:Имя>${DepartmentConfig.DepartmentWorkerFirstName}</УТ7:Имя>
			<УТ7:Отчество>${DepartmentConfig.DepartmentWorkerMiddleName}</УТ7:Отчество>
		</Специалист>
	</СлужебнаяИнформация>
</ЭДСФР>         